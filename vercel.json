{
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "yarn install",
  "devCommand": "yarn dev",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 